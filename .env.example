# Flask configuration
FLASK_SECRET_KEY=replace-with-random-string

# Google OAuth credentials
GOOGLE_CLIENT_ID=replace-with-google-client-id
GOOGLE_CLIENT_SECRET=replace-with-google-client-secret

# Google Photos Picker configuration
GOOGLE_PHOTOS_PICKER_API_KEY=replace-with-google-photos-picker-api-key

# Allow HTTP redirects for local development (remove or set to 0 in production)
OAUTHLIB_INSECURE_TRANSPORT=1

# Optional Mapbox token for additional map layers
MAPBOX_ACCESS_TOKEN=
